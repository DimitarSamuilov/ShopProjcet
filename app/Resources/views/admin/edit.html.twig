{% extends("base.html.twig") %}

{% block main %}
       {% if app.session.flashBag.has('error') %}
            <div class="alert alert-danger">
            {% for error in app.session.flashBag.get('error') %}
                {{ error }}
            {% endfor %}
        {{ form_start(form) }}
        {{ form_widget(form) }}
        {{ form_end(form) }}
{% endblock %}

